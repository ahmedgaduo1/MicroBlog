{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MicroBlogDb": "Server=.;Database=MicroBlogDb;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=True;TrustServerCertificate=True;"
  },
  "AzureBlobStorage": {
    "ConnectionString": "",
    "ContainerName": "microblog-images"
  },
  "LocalStorage": {
    "Path": "local-storage"
  },
  "JwtSettings": {
    "Key": "MicroBlogSecurityKeyWithAtLeast256BitsLength123456789012345678901234567890",
    "Issuer": "MicroBlog",
    "Audience": "MicroBlogUsers",
    "ExpiryMinutes": 60
  }
}
